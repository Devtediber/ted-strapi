        "metaTitle": {
            "type": "string",
            "required": false
        },
        "metaDescription": {
            "type": "text",
            "required": false
        },
        "productSlug": {
            "type": "string",
            "required": false
        },
        "keyProduct": {
            "type": "string",
            "required": false
        },
        "imageDesktop": {
            "type": "string",
            "required": false
